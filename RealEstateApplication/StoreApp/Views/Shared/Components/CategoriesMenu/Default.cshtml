@model IEnumerable<ProductFeatureGroup>
<div class="mt-3">
    <h6 class="lead">
        <i class="fa fa-solid fa-bars text-secondary"></i>
        Categories
    </h6>
</div>


<div class="my-4">
    <ul class="list-group">
        @foreach (var profe in Model)
        {
            <li class="list-group-item">
                <a asp-action="Index" asp-route-categoryId="@profe.id">
                    @profe.value

                </a>
            </li>
        }

    </ul>

</div>